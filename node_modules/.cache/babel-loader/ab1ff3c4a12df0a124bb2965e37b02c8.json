placeholder\"\n                    :regex=\"regex\"\n                    />\n            </div>\n                 \n      </div>             \n  </div>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            content: ''\n        }\n    },\n    props: ['label', 'type', 'placeholder', 'regex'],\n    methods: {\n        handlerRegex() {\n            const rule = new RegExp(this.regex)   \n            if(rule.test(this.content)){\n                this.$emit('inputChange', this.content)                         \n            }\n        }\n    },\n    watch: {\n        content() {\n            this.handlerRegex()\n            \n        }\n    },\n}\n</script>\n\n<style>\n    .login-text .user-info {\n        display: flex;\n        padding: 10px 0;\n        border-bottom: 1px solid lightgray;\n         \n    }\n    label {\n        display: block;\n        margin-left: 20px;\n    }\n    input {\n        width: 100%;\n        margin-left: 50px;\n        outline-style: none;\n        border: none;\n        font-size: 14px;\n        \n    }\n</style>"],"sourceRoot":"src/components/register"}]}